{
	"name": "Radiopaedia to Anki",
	"version": "0.1",
	"description": "Allows quick creation of Anki notes from Radiopaedia, using AnkiConnect",
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"content_scripts": [
	  {
	    "matches": ["https://radiopaedia.org/articles/*"],
	    "js": ["contentScriptRadiopaedia.js"]
	  }
	],
	"options_ui": {
	  "page": "options.html",
	  "open_in_tab": false
	},
	"permissions": [
		"http://localhost:8765/",
		"storage"
	],
	"manifest_version": 2
}